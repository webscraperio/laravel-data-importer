{
    "name": "webscraperio/laravel-data-importer",
    "type": "library",
    "description": "Import data from webscraper.io into database",
    "keywords": [
        "WebScraperIO",
        "LaravelDataImporter"
    ],
    "homepage": "https://github.com/webscraperio/webscraper.io-laravel-data-importer",
    "license": "MIT",
    "authors": [
        {
            "name": "webscraper.io",
            "email": "info@webscraper.io",
            "homepage": "webscraper.io",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "WebScraperIO\\LaravelDataImporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebScraperIO\\LaravelDataImporter\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
